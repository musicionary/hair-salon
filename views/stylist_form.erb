<div id="index-banner" class="parallax-container">
  <div class="section no-pad-bot">
    <div class="container">
      <br><br>
      <h1 class="header center teal-text text-lighten-2">Add A Stylist</h1>
      <div class="row center">
        <a href="/stylists" id="download-button" class="btn-large waves-effect waves-light teal lighten-1">View Stylists</a>
      </div>
      <br><br>

    </div>
  </div>
  <div class="parallax"><img src="<%= url('img/pugwrap.jpg') %>" alt="Unsplashed background img 1"></div>
</div>

<div class="row container">
  <form class="col s12" action="/stylists" method="post">
    <div class="row">
      <div class="col s6">
        <label for="name">Name</label>
        <input type="text" placeholder="Name" name="name" id="name" class="validate">
      </div>
      <div class="col s6">
        <label for="station">Station</label>
        <input type="text" placeholder="Station" name="station" id="station" class="validate">
      </div>
    </div>
    <button class="btn waves-effect waves-light" type="submit">Submit
      <i class="material-icons right">send</i>
    </button>
  </form>
</div>
